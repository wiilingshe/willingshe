/*按钮*/
.btn-b{height: 100%;flex:1 0 50%;}
.btn-r{width:32%;background:#e71751;text-align:center;line-height:100rpx;}
.btn-l{width:68%;background:#eeeeee;padding:0 40rpx 0 65rpx;height: 100%;}
.btn-m{width: 100%;height: 85rpx;line-height: 85rpx;border-radius: 5rpx;text-align: center;}

.img-round{width:60rpx;height:60rpx;border-radius:50%;margin-right:20rpx;}
.img-round2{width:80rpx;height:80rpx;border-radius:50%;margin-right:20rpx;}

/*弹出框*/
.pupop{position: fixed;width: 100%;height: 100%;background: #fff;left: 0;top:0;z-index:100;box-sizing: border-box;}

.hid{display: none;}

.store_img{width: 165rpx;height:165rpx;}


