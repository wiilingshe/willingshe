page{
	background-color: #eee;
}
.list{
	display: flex;
	-webkit-display:flex;
	justify-content: flex-start;
	font-size: 30rpx;
	background-color: #fff;
	border-bottom: 2rpx solid #eee;
	width: 100%;
	height: 80rpx;
}
.list .left{
	line-height: 80rpx;
	width: 80rpx;
	margin-left: 30rpx;
}
.list input{
	height: 80rpx;
	width: 600rpx;
	font-size: 30rpx;
}
checkbox-group{
	margin-left: 30rpx;
	margin-top: 30rpx;
}
.addAddress{
	width: 100%;
	height: 90rpx;
	text-align: center;line-height: 90rpx;
	overflow: hidden;
	color: #fff;
	background-color: #e71751;
	position: fixed;
	bottom: 0;
	font-size: 30rpx;
}
.checkbox{
	font-size: 30rpx;
}
.selectqu{
	display: flex;
	-webkit-display:flex;
	justify-content: flex-end;
	height: 600rpx;
	background-color: #fff;
	position: fixed;
	bottom: 0;
	z-index: 2;
	width: 100%;
}
.selectqu scroll-view{
	height: 600rpx; 
}
.queding{
	width: 100%;
	height: 80rpx;
	position: fixed;
	bottom: 600rpx;
	background-color: #fff;
	border-bottom: 1px solid #eee;
	text-align: right;
	line-height: 80rpx;
	overflow: hidden;
}
.queding text{
	margin-right: 30rpx;
}
scroll-view view{
	margin-left: 40rpx;
}