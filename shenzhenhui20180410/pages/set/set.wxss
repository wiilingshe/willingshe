/* pages/set/set.wxss */
.logo_box{
    width: 100%;
    height: 400rpx;
    background: white;
    margin-bottom: 12rpx;
}
.logo_box image{
   width: 274rpx;
   height: 274rpx;
   margin-top: 44rpx;
   margin-left: 200rpx;
  
}
.s_content{
    width: 100%;
    height: 80rpx;
    background: #fff;
    border-bottom: 1px solid #e5e5e5;
}
.s_content  .s_text{
    font-size: 14px;
    color: #333;
    margin-left: 52rpx;
    margin-top: 16rpx;
    float: left;
}
.s_content::after {
    content: '';
    display: block;
    width: 20rpx;
    height: 20rpx;
    border: 2rpx solid #bbb;
    border-width: 2rpx 2rpx 0 0;
    transform: rotate(45deg);
    position: absolute;
    right: 15rpx;
    top: calc(50% - 5px);
}